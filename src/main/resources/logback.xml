<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <pattern>%d{HH:mm:ss.SSS} %-5p [%c] - %m%n</pattern>
    </encoder>
  </appender>
  <logger name="org.jbpm.executor.impl" level="warn"
          additivity="false">
    <appender-ref ref="stdout" />
  </logger>
  <root level="INFO">
    <appender-ref ref="stdout"/>
  </root>
</configuration>